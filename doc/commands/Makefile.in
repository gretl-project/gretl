	./reflow -m < es1.tmp > $(SHAREDIR)/$(GUI_CMDREF).es && \
	./xsltrans --gui --pango $(ES_SRC) > es1.tmp && \
	./reflow -m < es1.tmp > $(SHAREDIR)/$(GUI_HELP).es && \
	touch $@ && rm -f es1.tmp

