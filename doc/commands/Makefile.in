$(ES_CMDREF_STAMP): xsltrans reflow $(ES_SRC) $(CMDDTD) $(HLPXSL) $(XMLSTRS) $(CHAPREFS)
	./xsltrans --cmds --plain $(ES_SRC) > es1.tmp && \
	./reflow < es1.tmp > $(SHAREDIR)/$(CLI_CMDREF).es && \
