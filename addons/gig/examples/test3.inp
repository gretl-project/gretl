
set verbose off
include gig.gfn
open b-g.gdt

aristotle = gig_setup(Y, 1, const)

loop for i=1..4 --quiet
    garch 0 i ; Y --quiet
    native = $coeff
    gig_set_pq(&aristotle, 0, i)
    gig_estimate(&aristotle,0)
    gigish = aristotle.coeff
    printf "%10.4f\n", native ~ gigish[1:rows(native)]
endloop
