# Example n. 4

# EGARCH(1,1) with several conditional distributions. Results are
# stored and printed later.


set verbose off
include gig.gfn

open djclose
rr = 100*ldiff(djclose)

set stopwatch
bundle basemod = gig_setup(rr, 7, const)
loop for i=0..4 --quiet
    bundle b$i = basemod
    gig_set_dist(&b$i, i)
    gig_estimate(&b$i, 0)
endloop
t0 = $stopwatch

loop for i=0..4 --quiet
    gig_print(&b$i)
endloop
printf "\nElapsed time = %g seconds\n", t0
